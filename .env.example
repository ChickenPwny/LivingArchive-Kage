# Database Configuration
# Copy this file to .env and fill in your actual credentials
# The .env file is gitignored and will not be committed

# Database Host
DB_HOST=localhost

# Database User
DB_USER=postgres

# Database Password (REQUIRED - leave empty if not using PostgreSQL)
DB_PASSWORD=your_password_here

# Customer Eggs Database
CUSTOMER_EGGS_DB_NAME=customer_eggs
CUSTOMER_EGGS_DB_PORT=15440

# EggRecords Database
EGG_DB_NAME=ego
EGG_DB_PORT=5436

# Django Secret Key (optional - will use insecure default if not set)
# SECRET_KEY=your-secret-key-here
